{
  "name": "@kuranov/intervals-client",
  "version": "1.1.3",
  "description": "Type-safe TypeScript client for the Intervals.icu API",
  "license": "MIT",
  "author": "Anton Kuranov <ya.kuranov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuranov/intervals-icu-tools.git"
  },
  "homepage": "https://github.com/kuranov/intervals-icu-tools#readme",
  "bugs": {
    "url": "https://github.com/kuranov/intervals-icu-tools/issues"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "pnpm tsx scripts/test-raw-api.ts && pnpm tsx scripts/analyze-nulls.ts && pnpm tsx scripts/validate-schemas.ts",
    "api-report": "api-extractor run --local",
    "api-report:ci": "api-extractor run",
    "docs": "api-extractor run --local && api-documenter markdown -i temp -o docs/api",
    "prepublishOnly": "pnpm run typecheck && pnpm run test && pnpm run build"
  },
  "keywords": [
    "intervals",
    "intervals.icu",
    "cycling",
    "training",
    "fitness",
    "typescript",
    "api-client",
    "strava",
    "workout",
    "athlete"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.3",
  "dependencies": {
    "ky": "^1.7.4",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.28.6",
    "@microsoft/api-extractor": "^7.56.0",
    "@redocly/cli": "^2.14.1",
    "@types/node": "^22.10.2",
    "msw": "^2.7.0",
    "tsdown": "0.19.0-beta.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}

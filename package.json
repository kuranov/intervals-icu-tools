  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.3",
  "dependencies": {
    "ky": "^1.7.4",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@redocly/cli": "^2.14.1",
    "@types/node": "^22.10.2",
    "msw": "^2.7.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}

{
  "name": "intervals-icu-client",
  "version": "0.0.0",
  "description": "Type-safe TypeScript client for the Intervals.icu API",
  "license": "MIT",
  "author": "",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }

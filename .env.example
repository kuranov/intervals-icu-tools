# Intervals.icu API Credentials
# Copy this file to .env and fill in your values

# Your Intervals.icu API key
# Find it at: https://intervals.icu/settings (scroll to "Developer Settings")
INTERVALS_API_KEY=your_api_key_here

# Your athlete ID
# Find it in the URL when viewing your calendar: https://intervals.icu/athlete/YOUR_ID/calendar
INTERVALS_ATHLETE_ID=your_athlete_id_here

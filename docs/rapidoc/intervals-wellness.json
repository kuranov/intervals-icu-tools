  "paths": {
    "/api/v1/athlete/{id}/wellness/{date}": {
      "get": {
        "tags": [
          "Wellness"
        ],
        "summary": "Get wellness record for date (local ISO-8601 day)",
        "operationId": "getRecord",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "date",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Wellness"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "Wellness"
        ],
        "summary": "Update the wellness record for the date (ISO-8601)",
        "description": "Only fields provided are changed",
        "operationId": "updateWellness",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "date",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Wellness"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Wellness"
                }
              }
            }
          }
        }
      }
    },

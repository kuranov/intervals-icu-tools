                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/athlete/{id}/wellness{ext}": {
      "get": {
        "tags": [
          "Wellness"
        ],
        "summary": "List wellness records for date range (use .csv for CSV format)",
        "operationId": "listWellnessRecords",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ext",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "oldest",
            "in": "query",
            "description": "Local date of oldest record (ISO-8601)",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "newest",
            "in": "query",
            "description": "Local date of newest record (ISO-8601), inclusive",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "cols",
            "in": "query",
            "description": "Comma separated list of column names to include in CSV (default is all)",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "fields",
            "in": "query",
            "description": "Comma separated list of field names to include in the returned objects (default is all), also excludes null values",
            "required": false,
            "schema": {
              "uniqueItems": true,
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Wellness"
                  }
                }
              }
            }
          }
        }
      }
    }
